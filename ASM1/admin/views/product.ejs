<%- include ('header'); %>
<div class="container-fluid">
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">Danh Mục</h6>
            <div class="mt-3">
                <a href="../view/product/add.html" class="btn btn-success" data-toggle="modal" data-target="#addCategoryModal">
                    <i class="fas fa-plus-circle"></i>
                    Thêm
                </a>
            </div>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table
                    class="table table-bordered"
                    id="dataTable"
                    width="100%"
                    cellspacing="0"
                >
                    <thead>
                        <tr>
                            <th>ID</th>
                            <th>Loại</th>
                            <th>Tên Sản Phẩm</th>
                            <th>Hình Sản Phẩm</th>
                            <th>Giá</th>
                            <th>Chức năng</th>
                        </tr>
                    </thead>
                    <tbody id="get_product"></tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<%- include ('footer'); %>